    "gh-pages": "^3.1.0",
    "highlight.js": "^11.0.0",
    "highlightjs-graphql": "^1.0.1",
    "highlightjs-line-numbers.js": "^2.7.0",
    "meow": "^6.1.0",
    "node-fetch": "2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.5",
