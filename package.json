{
  "description": "leafyGreen UI Kit",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">= 14.0.0"
  },
  "keywords": [
    "mongodb",
    "ui",
    "kit",
    "components",
    "react",
    "uikit",
    "leafygreen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb/leafygreen-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mongodb/leafygreen-ui/issues"
  },
  "scripts": {
    "init": "yarn && yarn build",
    "create-package": "node ./scripts/create-package.js",
    "pre-build": "node ./scripts/pre-build.js",
    "build": "npm-run-all --serial \"build:* {@}\" --",
    "build:packages": "yarn ts-node ./scripts/build.ts",
    "build:ts": "yarn ts-node ./scripts/ts-build.ts",
    "check": "npm-run-all --parallel check:*",
    "check:prettier": "prettier --check \"**/*.{js,ts,tsx,json,md,yml}\"",
    "check:eslint": "eslint \"**/*.{js,ts,tsx}\"",
    "check:deps": "ts-node -O '{\"module\": \"commonjs\", \"resolveJsonModule\": true}' ./scripts/depcheck.ts",
    "check:builds": "yarn ts-node ./scripts/check-builds.ts",
    "check:ts": "tsc --project tsconfig.json",
    "check:package-json": "npmPkgJsonLint ./packages",
    "clean": "yarn clean:builds && yarn clean:modules",
    "clean:builds": "rm -rf packages/*/dist packages/*/tsconfig.tsbuildinfo ./tsconfig.tsbuildinfo packages/**/stories.js",
    "clean:modules": "rm -rf node_modules packages/*/node_modules ",
    "demo:storybook": "build-storybook && ./scripts/demo-storybook.sh",
    "docs:tsdoc": "yarn ts-node ./scripts/parse-tsdocs.ts",
    "docs:proptypes": "yarn ts-node ./scripts/proptypes.ts",
    "fix": "npm-run-all --parallel fix:*",
    "fix:prettier": "prettier --write \"**/*.{js,ts,tsx,json,md,yml}\"",
    "fix:eslint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "link": "./scripts/link.sh",
    "link:all": "lerna exec -- yarn link",
    "prepublishOnly": "yarn fix && yarn test && yarn build && yarn prepublish:ts-downlevel",
    "prepublish:ts-downlevel": "rm -rf packages/*/dist/ts3.4 && echo packages/*/dist | NODE_NO_WARNINGS=1 xargs -I{} -n1 -P8 downlevel-dts {} {}/ts3.4",
    "release": "yarn publish",
    "publish": "npm-run-all --serial publish:*",
    "publish:packages": "yarn changeset publish --public",
    "publish:storybook": "build-storybook && gh-pages --dist ./storybook-static",
    "slackbot": "yarn ts-node ./scripts/slackbot",
    "start": "yarn start:storybook",
    "start:all": "npm-run-all --parallel start:storybook",
    "start:storybook": "start-storybook -p 9001 --no-version-updates",
    "test": "yarn ts-node ./scripts/test.ts",
    "test:local": "yarn test",
    "test:ssr": "yarn test --ssr",
    "ts-node": "ts-node -P './tsconfig.json' -O '{\"module\": \"commonjs\"}' --",
    "unlink": "./scripts/unlink.sh",
    "watch": "yarn build -- -- --watch"
  },
