{
  "name": "leafygreen-ui",
  "version": "0.0.0",
  "description": "leafyGreen UI Kit",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 8.19.4"
  },
  "scripts": {
    "init": "pnpm install && pnpm build",
    "init17": "yarn && yarn build:cli && npx ts-node tools/test/scripts/install-react17.ts && yarn build",
    "create-package": "lg create",
    "prebuild": "pnpm build:cli",
    "build": "turbo run build tsc",
    "build:cli": "turbo run build tsc --filter=@lg-tools/cli",
    "build-storybook": "npx storybook build",
    "chromatic": "npx chromatic",
    "clean": "npm-run-all --parallel clean:*",
    "clean:builds": " pnpm recursive exec rm -rf ./{dist,tsconfig.tsbuildinfo,stories.js}",
    "clean:modules": "pnpm recursive exec -- rm -rf node_modules; rm -rf node_modules",
    "fix": "lg lint --fix",
    "link": "lg link",
    "lint": "lg lint",
    "prepublishOnly": "yarn build && turbo run docs",
    "publish": "yarn changeset publish --public",
    "slackbot": "lg slackbot release",
    "start": "npx storybook dev -p 9001 --no-version-updates",
    "test": "lg test",
    "unlink": "lg unlink",
    "validate": "lg validate"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@babel/core": "7.24.3",
    "@changesets/cli": "^2.26.0",
    "@lg-tools/build": "workspace:^",
    "@lg-tools/cli": "workspace:^",
    "@lg-tools/meta": "workspace:^",
    "@lg-tools/storybook-addon": "workspace:^",
    "@types/cross-spawn": "6.0.2",
    "@types/fs-extra": "11.0.1",
    "@types/lodash": "^4.14.170",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "chromatic": "^11.0.0",
    "cross-spawn": "7.0.3",
    "dotenv": "^10.0.0",
    "fs-extra": "11.1.1",
    "gh-pages": "^3.1.0",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.17",
    "turbo": "^1.12.3",
    "typescript": "~4.9.0"
  },
  "resolutions": {
    "caniuse-lite": "^1.0.30001585",
    "jackspeak": "2.1.1"
  },
  "workspaces": [
    "chat/*",
    "packages/*",
    "tools/*"
  ],
  "keywords": [
    "mongodb",
    "ui",
    "kit",
    "components",
    "react",
    "uikit",
    "leafygreen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb/leafygreen-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mongodb/leafygreen-ui/issues"
  }
}
