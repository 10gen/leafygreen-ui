{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "init": {},
    "install": {},
    "build": {
      "dependsOn": ["^build"]
    },
    "clean": {},
    "fix": {},
    "test": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "publish": {
      "dependsOn": ["build", "test", "lint"]
    }
  }
}
