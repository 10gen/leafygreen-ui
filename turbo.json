{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "prebuild": {
      "dependsOn": ["^prebuild"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "tsc": {
      "dependsOn": ["^tsc", "^build"],
      "outputs": ["dist/**/*.d.ts", "tsconfig.tsbuildinfo"]
    }
  }
}
