{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "tsc", "prebuild"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"]
    },
    "tsc": {
      "dependsOn": ["^tsc"],
      "outputs": ["{projectRoot}/dist"]
    },
    "prebuild": {
      "dependsOn": ["^prebuild"],
      "outputs": ["{projectRoot}/dist"]
    }
  }
}
