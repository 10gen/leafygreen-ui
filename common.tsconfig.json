{
  "compilerOptions": {
    "target": "ES5",
    "jsx": "react",
    "allowJs": false,
    "pretty": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@leafygreen-ui/*": ["packages/*/src"]
    }
  }
}
